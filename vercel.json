{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/app",
      "destination": "/static/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
